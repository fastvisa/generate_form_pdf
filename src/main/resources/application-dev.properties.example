# Development Configuration Example
# Copy this file to application-dev.properties and set your actual values

# AWS Configuration
aws.accessKey=YOUR_AWS_ACCESS_KEY_HERE
aws.secretKey=YOUR_AWS_SECRET_KEY_HERE
aws.s3bucket.name=your-development-bucket-name
aws.s3bucket.region=us-east-1

# Optional: Enable debug logging in development
logging.level.com.fastvisa=DEBUG
logging.level.org.springframework.web=DEBUG

# Optional: Show SQL queries in development
spring.jpa.show-sql=true
spring.jpa.properties.hibernate.format_sql=true